<?xml version="1.0" encoding="UTF-8"?>
<doxygenlayout version="1.0">
  <!-- Complete Doxygen Layout optimized for Doxygen + Awesome -->

  <!-- Main navigation -->
  <navindex>
    <tab type="mainpage"   visible="yes" title="Home"/>
    <tab type="pages"      visible="yes" title="Pages" intro=""/>
    <tab type="topics"     visible="yes" title="Topics" intro=""/>

    <tab type="modules"    visible="yes" title="Modules" intro="">
      <tab type="modulelist"     visible="yes" title="Module List" intro=""/>
      <tab type="modulemembers"  visible="yes" title="Module Members" intro=""/>
    </tab>

    <tab type="namespaces" visible="yes" title="Namespaces">
      <tab type="namespacelist"    visible="yes" title="Namespace List" intro=""/>
      <tab type="namespacemembers" visible="yes" title="Namespace Members" intro=""/>
    </tab>

    <tab type="concepts"   visible="yes" title="Concepts"/>

    <tab type="interfaces" visible="yes" title="Interfaces">
      <tab type="interfacelist"    visible="yes" title="Interface List" intro=""/>
      <tab type="interfaceindex"   visible="$ALPHABETICAL_INDEX" title="Interface Index"/>
      <tab type="interfacehierarchy" visible="yes" title="Interface Hierarchy" intro=""/>
    </tab>

    <tab type="classes"    visible="yes" title="Classes">
      <tab type="classlist"        visible="yes" title="Class List" intro=""/>
      <tab type="classindex"       visible="$ALPHABETICAL_INDEX" title="Class Index"/>
      <tab type="hierarchy"        visible="yes" title="Hierarchy" intro=""/>
      <tab type="classmembers"     visible="yes" title="Class Members" intro=""/>
    </tab>

    <tab type="structs"    visible="yes" title="Structs">
      <tab type="structlist"       visible="yes" title="Struct List" intro=""/>
      <tab type="structindex"      visible="$ALPHABETICAL_INDEX" title="Struct Index"/>
    </tab>

    <tab type="exceptions" visible="yes" title="Exceptions">
      <tab type="exceptionlist"     visible="yes" title="Exception List" intro=""/>
      <tab type="exceptionindex"    visible="$ALPHABETICAL_INDEX" title="Exception Index"/>
      <tab type="exceptionhierarchy" visible="yes" title="Exception Hierarchy" intro=""/>
    </tab>

    <tab type="files"      visible="yes" title="Files">
      <tab type="filelist"  visible="yes" title="File List" intro=""/>
      <tab type="globals"   visible="yes" title="Globals" intro=""/>
    </tab>

    <tab type="examples"   visible="yes" title="Examples" intro=""/>
    <tab type="index"      visible="$ALPHABETICAL_INDEX" title="Alphabetical Index"/>
  </navindex>

  <!-- Class page -->
  <class>
    <briefdescription visible="yes"/>
    <includes          visible="$SHOW_HEADERFILE"/>
    <inheritancegraph  visible="yes"/>
    <collaborationgraph visible="no"/>
    <memberdecl>
      <nestedclasses            visible="yes" title="Nested Classes"/>
      <publictypes              title="Public Types"/>
      <services                 title="Services"/>
      <interfaces               title="Interfaces"/>
      <publicslots              title="Public Slots"/>
      <signals                  title="Signals"/>
      <publicmethods            title="Public Methods"/>
      <publicstaticmethods      title="Static Public Methods"/>
      <publicattributes         title="Public Attributes"/>
      <publicstaticattributes   title="Static Public Attributes"/>
      <protectedtypes           title="Protected Types"/>
      <protectedslots           title="Protected Slots"/>
      <protectedmethods         title="Protected Methods"/>
      <protectedstaticmethods   title="Static Protected Methods"/>
      <protectedattributes      title="Protected Attributes"/>
      <protectedstaticattributes title="Static Protected Attributes"/>
      <packagetypes             title="Package Types"/>
      <packagemethods           title="Package Methods"/>
      <packagestaticmethods     title="Static Package Methods"/>
      <packageattributes        title="Package Attributes"/>
      <packagestaticattributes  title="Static Package Attributes"/>
      <properties               title="Properties"/>
      <events                   title="Events"/>
      <privatetypes             title="Private Types"/>
      <privateslots             title="Private Slots"/>
      <privatemethods           title="Private Methods"/>
      <privatestaticmethods     title="Static Private Methods"/>
      <privateattributes        title="Private Attributes"/>
      <privatestaticattributes  title="Static Private Attributes"/>
      <friends                  title="Friends"/>
      <related                  title="Related" subtitle=""/>
      <membergroups             visible="yes"/>
    </memberdecl>
    <detaileddescription title="Detailed Description"/>
    <memberdef>
      <inlineclasses title="Inline Classes"/>
      <typedefs      title="Typedefs"/>
      <enums         title="Enumerations"/>
      <services      title="Services"/>
      <interfaces    title="Interfaces"/>
      <constructors  title="Constructors"/>
      <functions     title="Functions"/>
      <related       title="Related Items"/>
      <variables     title="Variables"/>
      <properties    title="Properties"/>
      <events        title="Events"/>
    </memberdef>
    <allmemberslink visible="yes"/>
    <usedfiles     visible="$SHOW_USED_FILES"/>
    <authorsection visible="yes"/>
  </class>

  <!-- Namespace page -->
  <namespace>
    <briefdescription visible="yes"/>
    <memberdecl>
      <nestednamespaces visible="yes" title="Nested Namespaces"/>
      <constantgroups  visible="yes" title="Constant Groups"/>
      <interfaces      visible="yes" title="Interfaces"/>
      <classes         visible="yes" title="Classes"/>
      <concepts        visible="yes" title="Concepts"/>
      <structs         visible="yes" title="Structs"/>
      <exceptions      visible="yes" title="Exceptions"/>
      <typedefs        title="Typedefs"/>
      <sequences       title="Sequences"/>
      <dictionaries    title="Dictionaries"/>
      <enums           title="Enumerations"/>
      <functions       title="Functions"/>
      <variables       title="Variables"/>
      <properties      title="Properties"/>
      <membergroups    visible="yes"/>
    </memberdecl>
    <detaileddescription title="Detailed Description"/>
    <memberdef>
      <inlineclasses   title="Inline Classes"/>
      <typedefs        title="Typedefs"/>
      <sequences       title="Sequences"/>
      <dictionaries    title="Dictionaries"/>
      <enums           title="Enumerations"/>
      <functions       title="Functions"/>
      <variables       title="Variables"/>
      <properties      title="Properties"/>
    </memberdef>
    <authorsection visible="yes"/>
  </namespace>

  <!-- Concept page -->
  <concept>
    <briefdescription  visible="yes"/>
    <includes          visible="$SHOW_HEADERFILE"/>
    <definition        visible="yes" title="Definition"/>
    <detaileddescription title="Detailed Description"/>
    <authorsection     visible="yes"/>
  </concept>

  <!-- File page -->
  <file>
    <briefdescription visible="yes"/>
    <includes         visible="$SHOW_INCLUDE_FILES"/>
    <includegraph     visible="yes"/>
    <includedbygraph  visible="yes"/>
    <sourcelink       visible="yes"/>
    <memberdecl>
      <interfaces     visible="yes" title="Interfaces"/>
      <classes        visible="yes" title="Classes"/>
      <structs        visible="yes" title="Structs"/>
      <exceptions     visible="yes" title="Exceptions"/>
      <namespaces     visible="yes" title="Namespaces"/>
      <concepts       visible="yes" title="Concepts"/>
      <constantgroups visible="yes" title="Constant Groups"/>
      <defines        title="Macros"/>
      <typedefs       title="Typedefs"/>
      <sequences      title="Sequences"/>
      <dictionaries   title="Dictionaries"/>
      <enums          title="Enumerations"/>
      <functions      title="Functions"/>
      <variables      title="Variables"/>
      <properties     title="Properties"/>
      <membergroups   visible="yes"/>
    </memberdecl>
    <detaileddescription title="Detailed Description"/>
    <memberdef>
      <inlineclasses  title="Inline Classes"/>
      <defines        title="Macros"/>
      <typedefs       title="Typedefs"/>
      <sequences      title="Sequences"/>
      <dictionaries   title="Dictionaries"/>
      <enums          title="Enumerations"/>
      <functions      title="Functions"/>
      <variables      title="Variables"/>
      <properties     title="Properties"/>
    </memberdef>
    <authorsection visible="yes"/>
  </file>

  <!-- Group page -->
  <group>
    <briefdescription visible="yes"/>
    <groupgraph       visible="yes"/>
    <memberdecl>
      <nestedgroups   visible="yes" title="Subgroups"/>
      <modules        visible="yes" title="Modules"/>
      <dirs           visible="yes" title="Directories"/>
      <files          visible="yes" title="Files"/>
      <namespaces     visible="yes" title="Namespaces"/>
      <concepts       visible="yes" title="Concepts"/>
      <classes        visible="yes" title="Classes"/>
      <defines        title="Macros"/>
      <typedefs       title="Typedefs"/>
      <sequences      title="Sequences"/>
      <dictionaries   title="Dictionaries"/>
      <enums          title="Enumerations"/>
      <enumvalues     title="Enum Values"/>
      <functions      title="Functions"/>
      <variables      title="Variables"/>
      <signals        title="Signals"/>
      <publicslots    title="Public Slots"/>
      <protectedslots title="Protected Slots"/>
      <privateslots   title="Private Slots"/>
      <events         title="Events"/>
      <properties     title="Properties"/>
      <friends        title="Friends"/>
      <membergroups   visible="yes"/>
    </memberdecl>
    <detaileddescription title="Detailed Description"/>
    <memberdef>
      <pagedocs       />
      <inlineclasses  title="Inline Classes"/>
      <defines        title="Macros"/>
      <typedefs       title="Typedefs"/>
      <sequences      title="Sequences"/>
      <dictionaries   title="Dictionaries"/>
      <enums          title="Enumerations"/>
      <enumvalues     title="Enum Values"/>
      <functions      title="Functions"/>
      <variables      title="Variables"/>
      <signals        title="Signals"/>
      <publicslots    title="Public Slots"/>
      <protectedslots title="Protected Slots"/>
      <privateslots   title="Private Slots"/>
      <events         title="Events"/>
      <properties     title="Properties"/>
      <friends        title="Friends"/>
    </memberdef>
    <authorsection visible="yes"/>
  </group>

  <!-- C++20 Module page -->
  <module>
    <briefdescription visible="yes"/>
    <exportedmodules visible="yes"/>
    <memberdecl>
      <concepts     visible="yes" title="Concepts"/>
      <classes      visible="yes" title="Classes"/>
      <enums        title="Enumerations"/>
      <typedefs     title="Typedefs"/>
      <functions    title="Functions"/>
      <variables    title="Variables"/>
      <membergroups title="Member Groups"/>
    </memberdecl>
    <detaileddescription title="Detailed Description"/>
    <memberdecl>
      <files visible="yes"/>
    </memberdecl>
  </module>

  <!-- Directory page -->
  <directory>
    <briefdescription visible="yes"/>
    <directorygraph   visible="yes"/>
    <memberdecl>
      <dirs  visible="yes" />
      <files visible="yes" />
    </memberdecl>
    <detaileddescription title="Detailed Description"/>
  </directory>
</doxygenlayout>
